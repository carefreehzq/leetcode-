/*
输入: J = "aA", S = "aAAbbbb"
输出: 3
*/
class Solution {
public:
    int numJewelsInStones(string J, string S) {
        int result=0;
        for(int i=0;i<S.size();i++)
        {
            for(int j=0;j<J.size();j++)
                if(S[i]==J[j])
                    {
                        result++;
                        break;
                    }
        }
        return result;
    }
};
